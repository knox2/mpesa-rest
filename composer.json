{
    "name": "knox/mpesa-rest",
    "description": "Laravel Package implementation of the MPESA REST API",
    "type": "library",
    "authors": [
        {
            "name": "Timothy Radier",
            "email": "timothyradier@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Knox\\MPESA\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Knox\\MPESA\\MpesaServiceProvider"
            ],
            "aliases": {
                "MPESA": "Knox\\MPESA\\Facades\\MPESA"
            }
        }
    }
}
